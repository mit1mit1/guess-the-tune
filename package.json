{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dayjs": "^1.11.1",
    "immer": "^9.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.1",
    "react-scripts": "5.0.0",
    "tone": "14.7.77",
    "web-vitals": "^2.1.0",
    "zustand": "^4.0.0-rc.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.15.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^7.0.4",
    "jest-axe": "^6.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.1.2",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:pre-push": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "pretlint:check": "prettier \"src/**/*.{ts,tsx}\" && yarn run eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "pretlint:write": "prettier --write \"src/**/*.{ts,tsx}\" && yarn run eslint --fix \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn run pretlint:check"
  },
  "packageManager": "yarn@3.2.0"
}
