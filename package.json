{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.4.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "tone": "^14.7.77",
    "typescript": "^4.4.2",
    "vexflow": "^3.0.9",
    "web-vitals": "^2.1.0",
    "zustand": "^3.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:pre-push": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "pretlint:check": "prettier \"src/**/*.{ts,tsx}\" && eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "pretlint:write": "prettier --write \"src/**/*.{ts,tsx}\" && eslint --fix \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/vexflow": "^1.2.37",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "yarn run pretlint:check"
  }
}
